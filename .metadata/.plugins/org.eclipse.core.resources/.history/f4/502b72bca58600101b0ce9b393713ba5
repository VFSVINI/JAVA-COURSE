package fundamentos;

public class Temperatura {
	
	public static void main(String[] args) {
		
		// (째F - 32) X 5/9 = 째C
		
		double f = 150.5;
		final double ajuste = 32;
		final double m = 5.0/9.0;
		double c = (f - ajuste) * m;
		
		System.out.println(c+"째C");
		
		f = 300.5;
		c = (f - ajuste) * m;
		
		System.out.println(c+ "째C");
		
		
		
	}

}
